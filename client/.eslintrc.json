{
    "extends": "airbnb-base/legacy",
    "plugins": [
        "import"
    ],
    "parserOptions": {
        "ecmaVersion": 5,
        "sourceType": "script"
    },
    "env": {
      "browser": "true"
    },
    "globals": {
      "Vue": false,
      "axios": false,
      "Promise": false
    },
    "rules": {
        "func-names": "off",
        "no-multi-str": "off"
    }
}
